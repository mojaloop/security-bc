{
  "name": "@mojaloop/security-bc-authentication-svc",
  "version": "0.3.5",
  "description": "mojaloop security authentication service",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/security-bc/tree/main/packages/authentication-svc",
  "private": true,
  "mojaloop": {
    "publish_to_dockerhub": true
  },
  "bugs": {
    "url": "https://github.com/mojaloop/security-bc/issues#readme"
  },
  "contributors": [
    "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run service",
    "start:dev": "npm run build && PLATFORM_CONFIG_BASE_SVC_URL=http://localhost:3100 PRIVATE_CERT_PEM_FILE_PATH=dist/private.pem IAM_STORAGE_FILE_PATH=dist/authN_TempIAMStorageFile.json npm start",
    "service": "node dist/application/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "clean:dist": "rm -rf dist",
    "clean:npm": "rm -rf node_modules",
    "clean": "rm -rf dist node_modules",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "npm run build && npx eslint ./src",
    "lint:fix": "npm run build && npx eslint ./src --fix",
    "test:unit": "jest",
    "audit:resolve": "SHELL=sh resolve-audit",
    "audit:check": "SHELL=sh check-audit",
    "dep:check": "ncu -e 2",
    "dep:update": "ncu -u"
  },
  "dependencies": {
    "@mojaloop/logging-bc-client-lib": "^0.1.29",
    "@mojaloop/platform-configuration-bc-client-lib": "^0.3.7",
    "@mojaloop/platform-configuration-bc-public-types-lib": "~0.3.5",
    "@mojaloop/security-bc-public-types-lib": "*",
    "asn1": "^0.2.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-jose": "^2.2.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/asn1": "^0.2.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node-jose": "^1.1.13",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "npm-check-updates": "~16.14.6",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.4"
  }
}
