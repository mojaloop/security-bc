{
    "name": "@mojaloop/security-bc-key-management-svc",
    "version": "0.5.4",
    "description": "mojaloop security key management service",
    "license": "Apache-2.0",
    "homepage": "https://github.com/mojaloop/security-bc/tree/main/packages/key-management-svc",
    "private": true,
    "mojaloop": {
        "publish_to_dockerhub": true
    },
    "bugs": {
        "url": "https://github.com/mojaloop/security-bc/issues#readme"
    },
    "contributors": [
        "Pedro Sousa Barreto <pedrosousabarreto@gmail.com>"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "start": "npm run service",
        "start:dev": "npm run build && PUBLIC_CERT_STORAGE_PATH=dist/certs PRIVATE_CERT_PEM_FILE_PATH=dist/private.pem PUBLIC_CERT_PEM_FILE_PATH=dist/public.pem npm start",
        "service": "node dist/application/index.js",
        "build": "tsc",
        "watch": "tsc -w",
        "clean:dist": "rm -rf dist",
        "clean:npm": "rm -rf node_modules",
        "clean": "rm -rf dist node_modules",
        "pretest": "npm run build",
        "standard": "npm run lint",
        "lint": "npm run build && npx eslint ./src",
        "lint:fix": "npm run build && npx eslint ./src --fix",
        "test:unit": "jest",
        "audit:resolve": "SHELL=sh resolve-audit",
        "audit:check": "SHELL=sh check-audit",
        "dep:check": "ncu -e 2",
        "dep:update": "ncu -u"
    },
    "dependencies": {
        "@mojaloop/logging-bc-client-lib": "^0.5.6",
        "@mojaloop/platform-configuration-bc-client-lib": "^0.5.3",
        "@mojaloop/platform-configuration-bc-public-types-lib": "~0.5.2",
        "@mojaloop/platform-shared-lib-messaging-types-lib": "~0.5.5",
        "@mojaloop/platform-shared-lib-nodejs-kafka-client-lib": "~0.5.7",
        "@mojaloop/platform-shared-lib-public-messages-lib": "~0.5.11",
        "@mojaloop/security-bc-public-types-lib": "*",
        "asn1": "^0.2.6",
        "express": "^4.18.2",
        "ioredis": "~5.3.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "node-jose": "^2.2.0",
        "node-vault": "^0.10.2",
        "tslib": "^2.6.2",
        "mongodb": "~6.3.0"
    },
    "devDependencies": {
        "@types/asn1": "^0.2.4",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node-jose": "^1.1.13",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "npm-check-updates": "~16.14.15",
        "ts-jest": "^29.1.2",
        "typescript": "^4.6.4"
    },
    "engines": {
        "node": ">=20.10.0"
    }
}
